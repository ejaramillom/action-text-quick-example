<h1>Articles#new</h1>
  <%= form_with scope: :article, url: articles_path, local: true do |form| %>
    <p>
      <%= form.label :title %>
      <br>
      <%= form.text_field :title %>
    </p>
    <p>
      <%= form.label :text %>
      <br>
      <%= form.rich_text_area :text  %>
    </p>
    <p>
      <%= form.submit %>
      <br>
    </p>
  <% end %>
<p>End of form</p>
